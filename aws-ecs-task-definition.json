{
    "family": "portfolio-app-task",
    "containerDefinitions": [
      {
        "name": "app",
        "image": "portfolio-app",
        "cpu": 0,
        "portMappings": [
          {
            "containerPort": 80,
            "hostPort": 80,
            "protocol": "tcp"
          }
        ],
        "essential": true,
        "environment": [],
        "mountPoints": [],
        "volumesFrom": []
      }
    ],
    "executionRoleArn": "arn:aws:iam::123456789012:role/portfolio-ecs-task-execution-role",
    "networkMode": "awsvpc",
    "requiresCompatibilities": [
      "FARGATE"
    ],
    "cpu": "256",
    "memory": "512"
  }